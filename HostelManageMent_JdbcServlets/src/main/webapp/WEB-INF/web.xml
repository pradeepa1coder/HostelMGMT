<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>HostelManageMent_JdbcServlets</display-name>

	<welcome-file-list>
		<welcome-file>home.html</welcome-file>
	</welcome-file-list>





	<servlet>
		<servlet-name>myservlet</servlet-name>
		<servlet-class>org.jsp.FirstServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>myservlet</servlet-name>
		<url-pattern>/fs</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>myservlet1</servlet-name>
		<servlet-class>org.jsp.SaveData</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>myservlet1</servlet-name>
		<url-pattern>/save</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>myservlet2</servlet-name>
		<servlet-class>org.jsp.UpdateData</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>myservlet2</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>myservlet3</servlet-name>
		<servlet-class>org.jsp.FindById</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>myservlet3</servlet-name>
		<url-pattern>/findbyid</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>myservlet4</servlet-name>
		<servlet-class>org.jsp.FindAll</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>myservlet4</servlet-name>
		<url-pattern>/fetchall</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>myservlet5</servlet-name>
		<servlet-class>org.jsp.DeleteByID</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>myservlet5</servlet-name>
		<url-pattern>/deletebyid</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>myservlet6</servlet-name>
		<servlet-class>org.jsp.UpdateById</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>myservlet6</servlet-name>
		<url-pattern>/updatebyid</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>myservlet6.1</servlet-name>
		<servlet-class>org.jsp.UpdateById1</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>myservlet6.1</servlet-name>
		<url-pattern>/updatebyid1</url-pattern>
	</servlet-mapping>
</web-app>